#include <stdio.h>
#include <locale.h>

int main(){
    char *locale = setlocale(LC_ALL, "");
    float a = 0;
    float b = 0;
    printf("Введите a: ");
    scanf("%e", &a);
    printf("Введите b: ");
    scanf("%e", &b);
    printf("Сумма = %g \n", a+b);
    printf("Разность = %g \n", a-b);
    printf("Произведение квадратов = %g \n", a*a*b*b);
    printf("Частное квадратов = %g \n", (a*a)/(b*b));
    return 0;
}
